get_filename_component(zacc_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${zacc_CMAKE_DIR})

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET zacc::zacc)
    include("${zacc_CMAKE_DIR}/zaccTargets.cmake")
endif()

set(zacc_LIBRARIES zacc::zacc)